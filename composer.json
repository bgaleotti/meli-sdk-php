{
    "name": "bgaleotti/meli-sdk-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Galeotti"
        }
    ],
    "description": "MercadoLibre API client.",
    "autoload": {
        "psr-4": {
            "Meli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Meli\\": "src/spec"
        }
    },
    "require": {
        "php": "^7.0",
        "pagerfanta/pagerfanta": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/client-common": "^1.2"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.1"
    },
    "suggest": {
        "adrienbrault/pagerfanta-iterator": "Support traversing paginated resources."
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
